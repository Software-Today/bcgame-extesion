{
    "name": "Auto Reload Slack",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
      "default_title": "This is action"
    },
    "description" : "We can auto reload Slack Messanger.",
    "icons": {    
        "128": "images/logo.png",
        "16": "images/logo_16.png",
        "48": "images/logo_48.png" },
    "permissions": [
        "activeTab", 
        "scripting",
        "declarativeContent",
        "webNavigation",
        "nativeMessaging",
        "tabs",
        "storage"],
    "host_permissions": [
          "<all_urls>"
        ],
    "background": {
      "service_worker": "backgrondServices/freelancer.js"
    },
    "content_scripts": [ {
      "matches": ["https://*/*"],
      "js": ["js/jquery.js"]
    } ]
  }